def print_abacus(value):
    a,b,c,d,e,f,g,h,i,j='|00000*****   |','|00000****   *|','|00000***   **|','|00000**   ***|','|00000*   ****|','|00000   *****|','|0000   0*****|','|000   00*****|','|00   000*****|','|0   0000*****|'
    p=[a,b,c,d,e,f,g,h,i,j]
    i,f=10,0
    z=str(value)
    q=i-len(z)
    while q>0:
        print p[0]
        q=q-1
    while f<=(len(z)-1):
        
        
        print p[int(z[f])]
        f=f+1
    
       #
